{
  "name": "simple-react-native-web-full-stack",
  "version": "1.0.0",
  "description": "Template to make Express, Webpack, and Mongoose apps",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src â€”-out-dir dist",
    "dev": "nodemon --exec babel-node src/index.js",
    "production": "yarn build && cd dist && node index.js",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "flow",
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "test": "yarn lint && yarn typecheck && jest --testPathPattern __tests__ --coverage",
    "postinstall": "flow-typed install --ignoreDeps dev",
    "precommit": "yarn lint && yarn typecheck"
  },
  "author": "WDS Production House",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4",
    "mongoose": "^5.4.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "9.x",
    "eslint": "5",
    "eslint-config-react-app": "^3.0.6",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "flow-bin": "^0.92.0",
    "flow-typed": "^2.5.1",
    "nodemon": "^1.18.9"
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  }
}
